        </Subtask>
        <Subtask name="Take picture">
          <CameraSettings focalLength="100mm" aperture="f/8" shutterSpeed="1/250s" ISO="400"></CameraSettings>
        </Subtask>
      </Task>
      <Task name="Drive to corner2">
        <Subtask name="Drive to location2">
          <GPSLocation xsi:type="double" latitude="47.535" longitude="-122.129" />
          <TreeType>Oak</TreeType>
