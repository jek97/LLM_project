            </Task>
            <Task>
                <TaskID>2</TaskID>
                <TaskType>TakePicture</TaskType>
                <Parameters>
                    <Parameter name="Subject">Tree</Parameter>
                    <Parameter name="GPSLocation">(GeoJSON Point)</Parameter>
                    <Parameter name="TreeType">(Tree Type from GeoJSON)</Parameter>
                </Parameters>
            </Task>
        </Tasks>
        <!-- ... -->
        <Tasks>
            <Task>
                <TaskID>3</TaskID>
                <TaskType>DriveToLocation</TaskType>
                <Parameters>
                    <Parameter name="Destination">(Point Coordinates)</Parameter>
                </Parameters>
            </Task>
            <Task>
                <TaskID>4</TaskID>
                <TaskType>TakePicture</TaskType>
                <Parameters>
                    <Parameter name="Subject">Tree</Parameter>
                    <Parameter name="GPSLocation">(GeoJSON Point)</Parameter>
                    <Parameter name="TreeType">(Tree Type from GeoJSON)</Parameter>
                </Parameters>
            </Task>
