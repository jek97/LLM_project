<local:MissionPlan xmlns:local="http://example.com/local"
                    xsi:schemaLocation="http://example.com/local missionplan.xsd">

    <local:CompositeTaskInformation TaskDescription="Sample Mission Plan">
        <local:SequentialTasks>
            <local:DriveToWaypoint WaypointID="WP1" />
            <local:TakePicture />
            <local:ConditionalTask>
                <local:Condition>Temperature > 25</local:Condition>
                <local:TrueBranch>
                    <local:ReadTemperature />
                </local:TrueBranch>
                <local:FalseBranch>
                    <local:TakeCO2Sample />
                </local:FalseBranch>
            </local:ConditionalTask>
        </local:SequentialTasks>
    </local:CompositeTaskInformation>

    <local:Waypoints>
        <local:Waypoint ID="WP1" />
    </local:Waypoints>

</local:MissionPlan>
